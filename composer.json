{
    "name": "shopsys/frontend-api",
    "type": "library",
    "description": "Shopsys Framework Frontend API support",
    "keywords": [
        "api",
        "Shopsys Framework",
        "GraphQL",
        "SSFW"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\FrontendApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\FrontendApiBundle\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "overblog/graphql-bundle": "^0.12",
        "overblog/graphiql-bundle": "^0.1",
        "shopsys/form-types-bundle": "dev-tp-symfony4 as dev-master",
        "shopsys/framework": "dev-tp-symfony4 as dev-master",
        "shopsys/migrations": "dev-tp-symfony4 as dev-master",
        "shopsys/plugin-interface": "dev-tp-symfony4 as dev-master",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/routing": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "shopsys/coding-standards": "dev-tp-symfony4 as dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    }
}
